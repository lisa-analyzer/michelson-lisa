                 DUP ;
                 DUG 5 ;
                 { CDR ; CAR ; CAR } ;
                 PAIR ;
                 PAIR ;
                 PAIR ;
                 DUG 2 ;
                 DROP 2 }
               { PUSH mutez 0 ;
                 DIG 2 ;
                 DUP ;
                 DUG 3 ;
                 { CAR ; CAR ; CDR } ;
                 COMPARE ;
