                 DIG 3 ;
                 DUP ;
                 DUG 4 ;
                 { CAR ; CAR ; CAR } ;
                 DIG 4 ;
                 DUP ;
                 DUG 5 ;
                 { CAR ; CAR ; CDR } ;
                 ADD ;
                 PUSH unit Unit ;
                 TRANSFER_TOKENS ;
                 CONS ;
                 DIG 2 ;
                 DUP ;
                 DUG 3 ;
                 DUP ;
                 CDR ;
                 SWAP ;
                 CAR ;
                 DUP ;
                 CDR ;
                 SWAP ;
